{
  "name": "@flexdoc/backend",
  "version": "1.0.0",
  "description": "FlexDoc backend integration for NestJS and other frameworks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "copy-templates": "copyfiles -u 2 src/templates/**/*.njk dist/templates/"
  },
  "keywords": [
    "openapi",
    "swagger",
    "documentation",
    "api",
    "nestjs",
    "express",
    "typescript"
  ],
  "author": "FlexDoc Team",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    }
  },
  "dependencies": {
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/testing": "^11.1.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/nunjucks": "^3.2.6",
    "copyfiles": "^2.4.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}
