{
  "name": "@bluejeans/flexdoc-client",
  "version": "1.0.0",
  "description": "FlexDoc client-side UI components for API documentation",
  "type": "commonjs",
  "main": "dist/flexdoc.umd.js",
  "module": "dist/flexdoc.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/flexdoc.es.js",
      "require": "./dist/flexdoc.umd.js"
    },
    "./styles.css": {
      "import": "./dist/assets/index.css",
      "require": "./dist/assets/index.css"
    }
  },
  "style": "./dist/assets/index.css",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tsc --emitDeclarationOnly --outDir dist",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "openapi",
    "swagger",
    "documentation",
    "api",
    "ui",
    "react",
    "typescript"
  ],
  "author": "Vishnu R <vishnurajesh45@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.294.0",
    "prismjs": "^1.29.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^7.0.0"
  }
}

